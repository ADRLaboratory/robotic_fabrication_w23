<?xml version="1.0"?>
<!-- Kuka KR 6 R900 sixx Robot -->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kr6_base">
    <!-- <xacro:property name="prefix" value="kr6_base_"/> -->

    <!-- Import all Gazebo elements -->
    <!-- <xacro:include filename="package://kr6_base_description/urdf/kr6_base.gazebo"/> -->

    <!-- materials -->
    <material name="orange">
        <color rgba="0.949 0.3608 0.098 1"/>
    </material>

    <material name="gray">
        <color rgba="0.3 0.3 0.3 1"/>
    </material>

    <!-- links -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/base_link.stl"/>
            </geometry>
            <material name="gray"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/base_link.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="-0.04 0.00 0.11" rpy="0 0 0"/>
            <mass value="14.6"/>
            <inertia
                ixx="0.001" ixy="0.00" ixz="0.00"
                iyy="0.001" iyz="0.00"
                izz="0.001"/>
        </inertial>
    </link>
    
    <link name="link_1">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_1.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_1.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.00 0.00 0.10" rpy="0 0 0"/>
            <mass value="9"/>
            <inertia
                ixx="0.001" ixy="0.00" ixz="0.00"
                iyy="0.001" iyz="0.00"
                izz="0.001"/>
        </inertial>
    </link>
    
    <link name="link_2">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_2.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_2.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="-0.01 0.00 0.21" rpy="0 0 0"/>
            <mass value="14.5"/>
            <inertia
                ixx="0.001" ixy="0.00" ixz="0.00"
                iyy="0.001" iyz="0.00"
                izz="0.001"/>
        </inertial>
    </link>
    
    <link name="link_3">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_3.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_3.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.05 0.00 0.01" rpy="0 0 0"/>
            <mass value="6"/>
            <inertia
                ixx="0.001" ixy="0.00" ixz="0.00"
                iyy="0.001" iyz="0.00"
                izz="0.001"/>
        </inertial>
    </link>
    
    <link name="link_4">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_4.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_4.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.09 0.00 0.00" rpy="0 0 0"/>
            <mass value="3.7"/>
            <inertia
                ixx="0.001" ixy="0.00" ixz="0.00"
                iyy="0.001" iyz="0.00"
                izz="0.001"/>
        </inertial>
    </link>
    
    <link name="link_5">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_5.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_5.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.01 0.00 0.00" rpy="0 0 0"/>
            <mass value="1.1"/>
            <inertia
                ixx="0.0000008" ixy="0.00" ixz="0.00"
                iyy="0.0000005" iyz="0.00"
                izz="0.0000004"/>
        </inertial>
    </link>
    
    <link name="link_6">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/visual/link_6.stl"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://kr6_base_description/meshes/collision/link_6.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.01 0.00 0.00" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia
                ixx="0.000000004" ixy="0.00" ixz="0.00"
                iyy="0.00000003" iyz="0.00"
                izz="0.00000003"/>
        </inertial>
    </link>

    <!-- joints -->
    
    <joint name="joint_1" type="revolute">
        <origin xyz="0 0 0.230" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="link_1"/>
        <axis xyz="0 0 -1"/>
        <limit lower="${radians(-170)}" upper="${radians(170)}" effort="1000.0" velocity="${radians(360)}"/>
        <dynamics damping="0.7"/>
    </joint>

    <joint name="joint_2" type="revolute">
        <origin xyz="0.025 0 0.170" rpy="0 ${radians(90)} 0"/>
        <parent link="link_1"/>
        <child link="link_2"/>
        <axis xyz="0 1 0"/>
        <limit lower="${radians(-190)}" upper="${radians(45)}" effort="1000.0" velocity="${radians(300)}"/>
        <dynamics damping="0.7"/>
    </joint>
    <joint name="joint_3" type="revolute">
        <origin xyz="0 0 0.455" rpy="0 ${radians(-90)} 0"/>
        <parent link="link_2"/>
        <child link="link_3"/>
        <axis xyz="0 1 0"/>
        <limit lower="${radians(-120)}" upper="${radians(156)}" effort="1000.0" velocity="${radians(360)}"/>
        <dynamics damping="0.7"/>
    </joint>
    <joint name="joint_4" type="revolute">
        <origin xyz="0.215 0 0.025" rpy="0 0 0"/>
        <parent link="link_3"/>
        <child link="link_4"/>
        <axis xyz="-1 0 0"/>
        <limit lower="${radians(-185)}" upper="${radians(185)}" effort="1000.0" velocity="${radians(381)}"/>
        <dynamics damping="0.7"/>
    </joint>
    <joint name="joint_5" type="revolute">
        <origin xyz="0.205 0 0" rpy="0 0 0"/>
        <parent link="link_4"/>
        <child link="link_5"/>
        <axis xyz="0 1 0"/>
        <limit lower="${radians(-120)}" upper="${radians(120)}" effort="1000.0" velocity="${radians(388)}"/>
        <dynamics damping="0.7"/>
    </joint>
    <joint name="joint_6" type="revolute">
        <origin xyz="0.072 0 0" rpy="0 0 0"/>
        <parent link="link_5"/>
        <child link="link_6"/>
        <axis xyz="-1 0 0"/>
        <limit lower="${radians(-350)}" upper="${radians(350)}" effort="1000.0" velocity="${radians(615)}"/>
        <dynamics damping="0.7"/>
    </joint>

    <!-- world frame for Gazebo -->
    <link name="world"/>
    <joint name="fixed" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <!-- 'flange' frame -->
    <link name="flange"/>
    <joint name="joint_6-flange" type="fixed">
        <origin xyz="0.018 0 0" rpy="0 0 0"/>
        <parent link="link_6"/>
        <child link="flange"/>
    </joint>

    <!-- 'tool0' frame -->
    <link name="tool0"/>
    <joint name="flange-tool0" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="flange"/>
        <child link="tool0"/>
    </joint>

    <!-- Gazebo plugins -->
    <gazebo>
        <plugin filename="libign_ros2_control-system.so" name="ign_ros2_control::IgnitionROS2ControlPlugin">
            <robot_param>robot_description</robot_param>
            <robot_param_node>robot_state_publisher</robot_param_node>
            <parameters>$(find kr6_base_moveit_config)/config/ros2_controllers.yaml</parameters>
            <!-- <controller_manager_prefix_node_name>controller_manager</controller_manager_prefix_node_name> -->
        </plugin>
    </gazebo>

</robot>
